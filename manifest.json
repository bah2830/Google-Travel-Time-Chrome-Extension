{
    "manifest_version": 2,

    "name": "Travel Time",
    "description": "Travel time estimator using google distance matrix",
    "version": "0.1.2",

    "icons": {
        "16": "resources/img/icons/icon16.png",
        "48": "resources/img/icons/icon48.png",
        "128": "resources/img/icons/icon128.png"
    },

    "browser_action": {
        "default_icon": "resources/img/icons/icon128.png",
        "default_popup": "views/popup.html"
    },

    "background": {
        "scripts": [
            "resources/js/jquery-3.2.1.min.js",
            "resources/js/background.js"
        ]
    },

    "options_ui": {
        "page": "views/options.html",
        "chrome_style": true
    },

    "permissions": [
        "https://maps.googleapis.com/*",
        "alarms",
        "storage"
    ]
}